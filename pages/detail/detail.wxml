      </view>
    </view>
    <view class="row">
      <view class="column_head left">
        <text></text></view>
      <view class="column left">
        <text class="nowarp_space">首时间：10元/小时</text>
      </view>
    </view>
    <view class="row">
      <view class="column_head left">
        <text></text></view>
      <view class="column left">
        <text class="nowarp_space">次时间：10元/小时</text>
      </view>

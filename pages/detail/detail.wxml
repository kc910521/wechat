  <text>\n</text>
  <view class="row" style="padding-left: 15%;">
    <view class="column padding_sm">
      <button size="mini" type="primary" bindtap="toOrder">预约</button>
    </view>
    <view class="column padding_sm">
      <button size="mini" type="default">去这里</button>
    </view>
  </view>
</view>
